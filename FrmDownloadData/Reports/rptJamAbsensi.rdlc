  <Code>Public Function strConvertToTimeValue(objValue As Object) As String

    If objValue is nothing Then
        strConvertToTimeValue= "N/A"
    Else
        strConvertToTimeValue= Format(Hour(objValue.tostring()), "00") + ":" + Format(minute(objValue.tostring()), "00")

    End If

End Function

Public Function GetSelisihWaktu(objValue As Object, timeIn as Object) As double

    If timeIn is nothing Then
        GetSelisihWaktu= -1
    Else
        GetSelisihWaktu= TimeValue(objValue.ToString()).Subtract( TimeValue(timeIn.ToString())).TotalMinutes

    End If

End Function


